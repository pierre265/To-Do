{
    "version": 2,
    "builds": [
      {
        "src": "src/renderer/index.html",
        "use": "@vercel/static",
        "config": {
          "distDir": "dist"
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/src/renderer/index.html"
      }
    ]
  }
  